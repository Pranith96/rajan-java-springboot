
public class Demo {

	public static void main(String[] args) {
		int i = 10;
		String s = "Hello world";
		char ch = 'A';
		double d = 12.0d;
		long l = 123456778L;
		float f = 12.0f;
		
		System.out.println(i);
		System.out.println(i+ " " +s+ " " +ch+ " " +d+ " "+l+ " "+f);
		
	
	}
	
	public void hello() {
		System.out.println("Hello Welcome");
	}

}
